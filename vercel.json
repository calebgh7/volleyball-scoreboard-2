{
  "version": 2,
  "builds": [
    {
      "src": "server/index-simple.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/index-simple.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/server/index-simple.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "regions": ["iad1"],
  "github": {
    "enabled": true,
    "silent": true
  }
}
